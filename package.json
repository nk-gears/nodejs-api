    "@types/express": "^4.16.1",
    "@types/express-rate-limit": "^3.3.0",
    "@types/helmet": "^0.0.43",
    "@types/http-errors": "^1.6.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^11.13.6",
    "@types/uuid": "^3.4.4",
